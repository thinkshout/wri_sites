{#
/**
 * @file
 * Theme override for the Topics Block view.
 *
 * @see template_preprocess_views_view()
 */
#}
{%
  set classes = [
    'view-' ~ id|clean_class,
    'view-id-' ~ id,
    'view-display-id-' ~ display_id,
  ]
%}
<div{{ attributes.addClass(classes) }}>
  {% if header %}
    {{ header }}
  {% endif %}
  {% if rows %}
    {{ rows }}
  {% endif %}
</div>
