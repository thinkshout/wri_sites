{
  "name": "ts_wrin",
  "version": "1.0.0",
  "description": "CSS Grid Layout with Webpack",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.0.5",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "css-loader": "^6.8.1",
    "fsevents": "^2.3.2",
    "husky": "^2.2.0",
    "mini-css-extract-plugin": "^1.0.0",
    "postcss-loader": "^3.0.0",
    "prettier": "1.17.0",
    "pretty-quick": "^1.10.0",
    "sass": "^1.79.4",
    "sass-loader": "^13.3.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "preinstall": "npx check-node-version --node $(cat .nvmrc)",
    "build": "webpack --config webpack.config.js",
    "build:dev": "webpack --config webpack.config.js --mode development",
    "start": "webpack --watch",
    "start:dev": "webpack --watch --mode development",
    "pretty-quick": "pretty-quick",
    "prettier": "prettier --write sass/**/*.scss js/**/*.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --pattern '**/ts_wrin/**' --staged"
    }
  },
  "author": "ThinkShout, Inc",
  "license": "GPL-3.0"
}
