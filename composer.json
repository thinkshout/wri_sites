{
    "name": "wri/wri_sites",
    "description": "WRI Drupal 8 profile",
    "type": "drupal-profile",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "drupal/views_arg_order_sort",
                "type": "drupal-module",
                "version": "dev-8.x-1.x",
                "source": {
                    "type": "git",
                    "url": "https://git.drupalcode.org/project/views_arg_order_sort.git",
                    "reference": "5d13cfca5749294d127ebc06348827f775342b37"
                }
            }
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "extra": {
        "drupal-lenient": {
            "allowed-list": ["drupal/custom_robots"]
        },
        "patches": {
            "drupal/ckeditor": {
                "Changes to a block's text field not saved when ckeditor source mode is active https://www.drupal.org/project/ckeditor/issues/3095304": "https://git.drupalcode.org/project/ckeditor/-/merge_requests/10.diff"
            },
            "drupal/core": {
                "Dynamic Views Arguments via Tokens https://www.drupal.org/project/drupal/issues/1699378": "https://www.drupal.org/files/issues/2023-11-05/1699378-255.patch",
                "Add reusable option to inline block creation https://www.drupal.org/project/drupal/issues/2999491": "https://www.drupal.org/files/issues/2020-05-21/2999491--reusable-title-display--49.patch",
                "Hyphens and forward slashes (-/) break Views contextual filters https://www.drupal.org/node/672606": "https://www.drupal.org/files/issues/2022-09-27/672606-76.patch",
                "Views RSS feeds have validation issues https://www.drupal.org/project/drupal/issues/1337894": "https://www.drupal.org/files/issues/2022-11-28/1337894-37.patch",
                "Add missing enclosure to Views RSS - 2511878-30": "https://www.drupal.org/files/issues/2020-07-24/views-rss-fields-enclosure-2511878-30.patch",
                "Views Term ID has broken filters (\"All of\", \"Is none of\") and contextual filters \"allow multiple\" https://www.drupal.org/project/drupal/issues/2829178": "https://git.drupalcode.org/project/drupal/-/merge_requests/5321.patch",
                "Dialog drupalAutoButtons option should be respected on initial load https://www.drupal.org/node/2793343": "https://www.drupal.org/files/issues/2023-07-11/2793343-83.patch"
            },
            "drupal/content_snippets": {
                "Error: Cannot unset string offsets... https://www.drupal.org/project/content_snippets/issues/3268757": "https://git.drupalcode.org/issue/content_snippets-3268757/-/merge_requests/1.diff"
            },
            "drupal/custom_robots": {
                "Automated Drupal 10 compatibility fixes https://www.drupal.org/project/custom_robots/issues/3286869": "https://www.drupal.org/files/issues/2022-06-15/custom_robots.1.x-dev.rector.patch"
            },
            "drupal/diff": {
                "No local tasks on compare revisions page https://www.drupal.org/project/diff/issues/3169729": "https://www.drupal.org/files/issues/2020-09-08/diff_add-local-tasks-3169729-2.patch"
            },
            "drupal/menu_block": {
                "Add option to always render parent menu item https://www.drupal.org/project/menu_block/issues/3082445": "https://www.drupal.org/files/issues/2022-11-08/menu_block-render-parent-3082445-53.patch"
            },
            "drupal/paragraphs": {
                "Integrity constraint violation: 1048 Column 'langcode' cannot be null https://www.drupal.org/project/paragraphs/issues/2901390": "https://www.drupal.org/files/issues/2020-06-25/paragraphs-2901390-51.patch",
                "Paragraph access check using incorrect revision of its parent https://www.drupal.org/project/paragraphs/issues/3090200": "https://www.drupal.org/files/issues/2022-10-26/3084934-13_combine_3090200-22.patch"
            },
            "drupal/token": {
                "Add a [current-page:object:?] dynamic token https://www.drupal.org/project/token/issues/919760": "https://www.drupal.org/files/issues/2020-09-08/token--current_page_object_token--919760--83.patch",
                "Provide array of primitive property values on multivalue field https://www.drupal.org/project/token/issues/3115486": "https://www.drupal.org/files/issues/2020-12-11/token-provide_primitive_type_arrays_for_properties_of_multivalue_fields-3115486-12.patch"
            },
            "drupal/hierarchical_term_formatter": {
                "Use the toUrl to create the link (compatibility with Forum module)": "https://git.drupalcode.org/project/hierarchical_term_formatter/-/merge_requests/1.diff"
            },
            "drupal/views_bulk_edit": {
                "Fix Required fields to really not be required https://www.drupal.org/node/3024419": "https://www.drupal.org/files/issues/2020-11-20/3024419-invalid-form-control-24.patch"
            },
            "drupal/simple_popup_blocks": {
                "Warning: htmlspecialchars() expects parameter 1 to be string, array given https://www.drupal.org/files/issues/2022-03-09/simple_popup_blocks-n3267864-5.patch":"https://git.drupalcode.org/project/simple_popup_blocks/-/merge_requests/4.diff",
                "Uncaught TypeError: $(...).once is not a function with Drupal 10 https://www.drupal.org/project/simple_popup_blocks/issues/3350919": "https://www.drupal.org/files/issues/2023-08-22/once_deprecation-3350919-25.patch"
            },
            "drupal/views_arg_order_sort": {
                "Set as compatible with Drupal 10 but lock down version https://www.drupal.org/project/views_arg_order_sort/issues/3212018" : "web/profiles/contrib/wri_sites/patches/d10-upgrade-circumvent-3212018.diff"
            },
            "drupal/views_extras": {
                "Why does Views Extras require Drupal_Visitors to be part of the cookie name? https://www.drupal.org/project/views_extras/issues/3054202" : "https://www.drupal.org/files/issues/2024-01-03/3054202-11.patch"
            },
            "drupal/layout_builder_modal": {
                "incompatibility with layout_builder_st, contextual links broken https://www.drupal.org/project/layout_builder_modal/issues/3133695": "https://www.drupal.org/files/issues/2022-12-15/3133695-13.patch"
            }
        },
        "drupal-scaffold": {
            "locations": {
                "project-root": "./web",
                "web-root": "./web"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/private/scripts/quicksilver/{$name}/": [
                "type:quicksilver-script"
            ]
        }
    },
    "require": {
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.0",
        "drupal/access_unpublished": "^1.3",
        "drupal/address": "^2.0",
        "drupal/addtocal": "^3.0.0-beta1",
        "drupal/admin_toolbar": "^3.2",
        "drupal/allowed_formats": "^3.0",
        "drupal/asset_injector": "^2.9",
        "drupal/auto_entitylabel": "^3.0@beta",
        "drupal/block_list_override": "^1.0",
        "drupal/cer": "^5.0@beta",
        "drupal/ckeditor5_template": "^1.0",
        "drupal/ckeditor_accordion": "^2.0",
        "drupal/ckeditor_templates": "^1.2",
        "drupal/ckeditor_templates_ui": "^1.4",
        "drupal/classy": "^1.0",
        "drupal/config_partial_export": "^2.0@alpha",
        "drupal/content_snippets": "^2.0",
        "drupal/core-composer-scaffold": "^10.0",
        "drupal/core-recommended": "^10.0",
        "drupal/ctools": "^4.0",
        "drupal/custom_robots": "*",
        "drupal/diff": "^1.0",
        "drupal/ds": "^5.0.0",
        "drupal/editor_advanced_link": "^2.2",
        "drupal/element_class_formatter": "^2.0@beta",
        "drupal/entity_browser": "^2.6",
        "drupal/entity_class_formatter": "^2.0",
        "drupal/facets": "^2.0",
        "drupal/facets_pretty_paths": "^1.2",
        "drupal/field_group": "^3.2",
        "drupal/file_replace": "^1.2",
        "drupal/focal_point": "^2.0",
        "drupal/hierarchical_term_formatter": "^1.2",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/layout_builder_modal": "^1.1",
        "drupal/layout_builder_restrictions": "^2.12",
        "drupal/link_attributes": "^2.1",
        "drupal/media_library_edit": "^3.0",
        "drupal/media_responsive_thumbnail": "^1.2",
        "drupal/menu_block": "^1.8",
        "drupal/menu_position": "^1.0@beta",
        "drupal/metatag": "^2.0",
        "drupal/metatag_google_scholar": "^1.0",
        "drupal/oembed_lazyload": "^2.0",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.9",
        "drupal/redirect": "^1.7",
        "drupal/redis": "^1.5",
        "drupal/responsive_tables_filter": "^1.9",
        "drupal/scheduled_transitions": "^2.2",
        "drupal/schema_metatag": "^3.0",
        "drupal/search_api": "^1.28",
        "drupal/search_api_exclude_entity": "^2.0",
        "drupal/search_api_pantheon": "^8.0",
        "drupal/search_api_solr": "^4.2.1",
        "drupal/sendgrid_integration": "^2.1",
        "drupal/simple_popup_blocks": "^3.1",
        "drupal/simple_recaptcha": "^1.0@beta",
        "drupal/smart_date": "^3.4",
        "drupal/sortableviews": "^1.0@beta",
        "drupal/token": "^1.8",
        "drupal/token_filter": "^2.1",
        "drupal/twig_tweak": "^3.2",
        "drupal/twitter_api_block": "^4.0",
        "drupal/viewfield": "^3.0@beta",
        "drupal/views_arg_order_sort": "dev-8.x-1.x",
        "drupal/views_autosubmit": "^1.4",
        "drupal/views_bulk_edit": "^2.6",
        "drupal/views_bulk_operations": "^4.0",
        "drupal/views_extras": "^3.0",
        "drupal/webform": "^6.1",
        "drupal/xmlsitemap": "^1.2",
        "mglaman/composer-drupal-lenient": "*",
        "npm-asset/jquery.toc": "^0.4.0",
        "oomphinc/composer-installers-extender": "^2.0",
        "solarium/solarium": "^6.1.5",
        "thinkshout/distro_helper": "dev-logging-and-errors-15"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "composer/installers": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "oomphinc/composer-installers-extender": true,
            "mglaman/composer-drupal-lenient": true
        }
    },
    "require-dev": {
        "drupal/coder": "*",
        "drupal/config_devel": "*",
        "drupal/config_suite": "*",
        "drupal/devel": "*",
        "drupal/field_tools": "*",
        "drupal/stage_file_proxy": "*"
    }
}
