{
    "name": "wri/wri_sites",
    "description": "WRI Drupal 8 profile",
    "type": "drupal-profile",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "dinbror/blazy",
                "version": "1.8.2",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "blazy"
                },
                "source": {
                    "type": "git",
                    "url": "https://github.com/dinbror/blazy",
                    "reference": "1.8.2"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor/templates",
                "version": "4.15.0",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "ckeditor/plugins/templates"
                },
                "dist": {
                    "url": "https://download.ckeditor.com/templates/releases/templates_4.15.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor/templates",
                "version": "4.15.0",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "ckeditor/plugins/templates"
                },
                "dist": {
                    "url": "https://download.ckeditor.com/templates/releases/templates_4.15.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "extra": {
        "patches": {
            "drupal/core": {
                "Dynamic Views Arguments via Tokens https://www.drupal.org/project/drupal/issues/1699378":"https://www.drupal.org/files/issues/2020-10-29/1699378-210--on-89x.do-not-test.patch",
                "There is no way to delete or update file entities of other users incl. user/1":"https://www.drupal.org/files/issues/2019-12-12/2949017-59.patch",
                "Add reusable option to inline block creation https://www.drupal.org/project/drupal/issues/2999491": "https://www.drupal.org/files/issues/2020-05-21/2999491--reusable-title-display--49.patch",
                "Allow field blocks to display the configuration label when set in Layout Builder https://www.drupal.org/project/drupal/issues/3039185": "https://www.drupal.org/files/issues/2020-07-17/3039185-block_title-33.patch",
                "Hyphens and forward slashes (-/) break Views contextual filters https://www.drupal.org/node/672606": "https://www.drupal.org/files/issues/2021-03-03/672606-56.patch",
                "Taxonomy Name infinite loop https://www.drupal.org/project/drupal/issues/2693787": "https://git.drupalcode.org/project/drupal/-/merge_requests/234.diff",
                "Formatters for empty fields do not render with layout builder enabled": "https://www.drupal.org/files/issues/2020-01-24/3067982-8.patch",
                "Plugin blocks cannot set their own attributes when put in the layout": "https://www.drupal.org/files/issues/2020-04-11/plugin-block-attributes-fix-3077734-13.patch",
                "Views RSS feeds have validation issues https://www.drupal.org/project/drupal/issues/1337894": "https://www.drupal.org/files/issues/d8-rss-validation-atom-link-missing-1337894-25.patch",
                "Add missing enclosure to Views RSS - 2511878-30": "https://www.drupal.org/files/issues/2020-07-24/views-rss-fields-enclosure-2511878-30.patch"
            },
            "drupal/config_suite": {
                "Syntax error in ConfigSuiteImportSubscriber https://www.drupal.org/project/config_suite/issues/3189614": "https://git.drupalcode.org/project/config_suite/-/merge_requests/1.diff"
            },
            "drupal/diff": {
                "No local tasks on compare revisions page https://www.drupal.org/project/diff/issues/3169729": "https://www.drupal.org/files/issues/2020-09-08/diff_add-local-tasks-3169729-2.patch"
            },
            "drupal/menu_block": {
                "https://www.drupal.org/project/menu_block/issues/3082445 https://www.drupal.org/project/menu_block/issues/3082445": "https://www.drupal.org/files/issues/2019-09-20/menu_block-render-parent-menu-item-3082445-4.patch"
            },
            "drupal/sfc": {
                "Duplicate content when pulling SFC layout into views https://www.drupal.org/project/sfc/issues/3175000": "https://www.drupal.org/files/issues/2020-10-06/duplicate_ds_content-3175000-2.patch"
            },
            "drupal/paragraphs": {
                "Integrity constraint violation: 1048 Column 'langcode' cannot be null https://www.drupal.org/project/paragraphs/issues/2901390": "https://www.drupal.org/files/issues/2020-06-25/paragraphs-2901390-51.patch"
            },
            "drupal/token": {
                "Add a [current-page:object:?] dynamic token https://www.drupal.org/project/token/issues/919760": "https://www.drupal.org/files/issues/2020-09-08/token--current_page_object_token--919760--83.patch",
                "Provide array of primitive property values on multivalue field https://www.drupal.org/project/token/issues/3115486": "https://www.drupal.org/files/issues/2020-12-11/token-provide_primitive_type_arrays_for_properties_of_multivalue_fields-3115486-12.patch"
            },
            "drupal/hierarchical_term_formatter": {
                "Use the toUrl to create the link (compatibility with Forum module)": "https://git.drupalcode.org/project/hierarchical_term_formatter/-/merge_requests/1.diff"
            },
            "drupal/search_api_pantheon": {
                "Fixing More Like This for D8 on Pantheon https://github.com/pantheon-systems/search_api_pantheon/pull/69": "https://patch-diff.githubusercontent.com/raw/pantheon-systems/search_api_pantheon/pull/69.diff"
            },
            "drupal/textfield_counter": {
                "'Prevent form submission when character limit exceeded' option does not work https://www.drupal.org/project/textfield_counter/issues/3004457": "https://www.drupal.org/files/issues/2018-10-04/textfield_counter-prevent_form_submission_does_not_work-3004457-2.patch",
                "Issue #3182675: Incompatibilities with Claro theme, which duplicates the field selector class": "https://git.drupalcode.org/project/textfield_counter/-/merge_requests/1.patch"
            },
            "drupal/views_bulk_edit": {
                "Fix Required fields to really not be required https://www.drupal.org/node/3024419": "https://www.drupal.org/files/issues/2020-11-20/3024419-invalid-form-control-24.patch"
            },
            "drupal/simple_popup_blocks": {
                "Modal hops user down the page to the first text input https://www.drupal.org/project/simple_popup_blocks/issues/3202060#comment-14021232": "https://git.drupalcode.org/project/simple_popup_blocks/-/merge_requests/2.diff"
            }
        }
    },
